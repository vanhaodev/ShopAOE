					<% }else if(typeof(user) !== 'undefined' && !user.isAuthenticated){ %>
						<a href="/verify-email" class="btn">Kích hoạt email để thanh toán</a>
						<% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
					<% }); %>
